{
  "name": "reactive-json-form-ng",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dist-lib-init": "rm rf dist/reactive-json-form-ng && git worktree add dist/reactive-json-form-ng deploy",
    "build-lib": "rm -rf dist/reactive-json-form-ng/* && ng build reactive-json-form-ng --prod",
    "deploy-lib": "cd dist/reactive-json-form-ng && git add --all && git commit -m 'release' && git push",
    "precise-commits": "precise-commits"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precise-commits"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.3",
    "@angular/cdk": "^6.4.6",
    "@angular/common": "^6.1.3",
    "@angular/compiler": "^6.1.3",
    "@angular/core": "^6.1.3",
    "@angular/forms": "^6.1.3",
    "@angular/http": "^6.1.3",
    "@angular/material": "^6.4.6",
    "@angular/platform-browser": "^6.1.3",
    "@angular/platform-browser-dynamic": "^6.1.3",
    "@angular/router": "^6.1.3",
    "core-js": "^2.5.4",
    "espression": "^1.1.1",
    "espression-rx": "^0.1.2",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.4",
    "@angular-devkit/build-ng-packagr": "~0.7.4",
    "@angular/cli": "~6.1.4",
    "@angular/compiler-cli": "^6.1.3",
    "@angular/language-service": "^6.1.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^4.4.4",
    "husky": "^0.14.3",
    "ng-packagr": "^4.1.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.14.2",
    "ts-node": "~7.0.1",
    "tsickle": "^0.32.1",
    "tslib": "^1.7.1",
    "tslint": "^5.11.0",
    "typescript": "~2.9.2"
  }
}
